<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeScreenActivity">

    <Button
        android:id="@+id/homescreenSignOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:text="SignOut"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/homescreenGoToSteps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Steps"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="146dp" />

    <TextView
        android:id="@+id/homeScreenName"
        android:layout_width="212dp"
        android:layout_height="137dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/homescreenSignOut"
        app:layout_constraintTop_toTopOf="@+id/homescreenSignOut" />

    <Button
        android:id="@+id/challenges"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="challenges"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="85dp" />

    <Button
        android:id="@+id/uitest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UI Test"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="213dp" />

</androidx.constraintlayout.widget.ConstraintLayout>